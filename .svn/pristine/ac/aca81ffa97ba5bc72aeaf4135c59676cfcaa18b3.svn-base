package model.metadata.xml.dataquality;

/**
 *
 * @author wallet
 */
public class ValueUnit {
    
    private String subparent;
    private String current="gmd:valueUnit.gml:BaseUnit";
    private String unitDefinition="gml:UnitDefinition.gml:description";
    private String remarks="gml:remarks";
    private String quantityType="gml:quantityType";
    private String quantityTypeReference="gml:quantityTypeReference";
    private String catalogSymbol="gml:catalogSymbol";
    private String description="gml:description";
    private String descriptionReference="gml:descriptionReference";
    private String metaDataProperty="gml:metaDataProperty";
    private String identifier="gml:identifier";
    private String unitsSystem="gml:unitsSystem";

    public ValueUnit(String parent) {
        subparent = parent+"."+current;
    }

    public String getUnitDefinition() {
        return unitDefinition;
    }

    public String getRemarks() {
        return remarks;
    }

    public String getQuantityType() {
        return quantityType;
    }

    public String getQuantityTypeReference() {
        return quantityTypeReference;
    }

    public String getCatalogSymbol() {
        return catalogSymbol;
    }

    public String getDescription() {
        return description;
    }

    public String getDescriptionReference() {
        return descriptionReference;
    }

    public String getMetaDataProperty() {
        return metaDataProperty;
    }

    public String getIdentifier() {
        return identifier;
    }

    public String getUnitsSystem() {
        return unitsSystem;
    }

    public String UnitDefinition() {
        return subparent+"."+unitDefinition;
    }

    public String Remarks() {
         return subparent+"."+remarks;
    }

    public String QuantityType() {
         return subparent+"."+quantityType;
    }

    public String QuantityTypeReference() {
        return subparent+"."+quantityTypeReference;
    }

    public String CatalogSymbol() {
         return subparent+"."+catalogSymbol;
    }

    public String Description() {
         return subparent+"."+description;
    }

    public String DescriptionReference() {
         return subparent+"."+descriptionReference;
    }

    public String MetaDataProperty() {
         return subparent+"."+metaDataProperty;
    }

    public String Identifier() {
         return subparent+"."+identifier;
    }

    public String UnitsSystem() {
        return subparent+"."+unitsSystem;
    }
    
    
}
