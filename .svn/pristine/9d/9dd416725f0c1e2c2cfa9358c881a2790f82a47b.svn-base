    private void changePanel(JPanel oldPanel, JPanel panel) {
        contentPanel.remove(oldPanel);
        contentPanel.add(panel, BorderLayout.CENTER);
        contentPanel.invalidate();
        contentPanel.updateUI();
        doLayout();
        update(getGraphics());
    }


